{
  "kind": "collectionType",
  "collectionName": "newsletter_signups",
  "info": {
    "singularName": "newsletter-signup",
    "pluralName": "newsletter-signups",
    "displayName": "newsletter-signups"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "email": {
      "type": "string",
      "unique": true
    },
    "source": {
      "type": "string"
    },
    "statusSubscription": {
      "type": "enumeration",
      "default": "subscribed",
      "enum": [
        "pending",
        "subscribed",
        "unsubscribed"
      ]
    },
    "confirmTokenHash": {
      "type": "string"
    },
    "confirmTokenExpires": {
      "type": "datetime"
    },
    "confirmedAt": {
      "type": "datetime"
    },
    "optin": {
      "type": "integer"
    }
  }
}
