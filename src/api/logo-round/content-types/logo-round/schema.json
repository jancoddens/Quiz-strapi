{
  "kind": "collectionType",
  "collectionName": "logo_rounds",
  "info": {
    "singularName": "logo-round",
    "pluralName": "logo-rounds",
    "displayName": "Logo round"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "logos": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::logo.logo",
      "inversedBy": "logo_rounds"
    },
    "title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "type": "uid",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "targetField": "title",
      "required": true
    },
    "description": {
      "type": "blocks",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "visibility": {
      "type": "enumeration",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": "public",
      "enum": [
        "public",
        "unlisted",
        "private"
      ]
    },
    "seo": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.seo",
      "repeatable": false
    },
    "timePerQuestion": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "totalTimeLimit": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "estimatedTime": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "game_sessions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::game-session.game-session",
      "mappedBy": "logo_round"
    }
  }
}
