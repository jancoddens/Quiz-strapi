{
  "kind": "collectionType",
  "collectionName": "quizzes",
  "info": {
    "singularName": "quiz",
    "pluralName": "quizzes",
    "displayName": "Quiz"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "slug": {
      "type": "uid",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "targetField": "title",
      "required": true
    },
    "description": {
      "type": "blocks",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "isPublic": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": true
    },
    "mode": {
      "type": "enumeration",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "default": "short",
      "enum": [
        "short",
        "live",
        "practice",
        "example"
      ]
    },
    "estimatedTime": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "visibility": {
      "type": "enumeration",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": "public",
      "enum": [
        "public",
        "unlisted",
        "private"
      ]
    },
    "owner": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "coverImage": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "seo": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.seo",
      "repeatable": false
    },
    "game_sessions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::game-session.game-session",
      "mappedBy": "quiz"
    },
    "timePerQuestion": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "totalTimeLimit": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "download": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "rounds": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::round.round",
      "inversedBy": "quizzes"
    },
    "date": {
      "type": "date",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "featured": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "quizzes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::quiz.quiz",
      "inversedBy": "related_quizzes"
    },
    "related_quizzes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::quiz.quiz",
      "mappedBy": "quizzes"
    },
    "difficulty": {
      "type": "enumeration",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": "CD",
      "enum": [
        "A",
        "AB",
        "B",
        "BC",
        "C",
        "CD",
        "D"
      ]
    }
  }
}
