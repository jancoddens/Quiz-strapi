{
  "kind": "collectionType",
  "collectionName": "rounds",
  "info": {
    "singularName": "round",
    "pluralName": "rounds",
    "displayName": "Round"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "slug": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "description": {
      "type": "blocks",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "isPublic": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": true
    },
    "estimatedTime": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "visibility": {
      "type": "enumeration",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "enum": [
        "public",
        "unlisted",
        "private"
      ]
    },
    "owner": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "rounds"
    },
    "seo": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.seo",
      "repeatable": false
    },
    "game_sessions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::game-session.game-session",
      "mappedBy": "round"
    },
    "timePerQuestion": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "totalTimeLimit": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "download": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "questions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::question.question",
      "mappedBy": "rounds"
    },
    "quizzes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::quiz.quiz",
      "mappedBy": "rounds"
    }
  }
}
