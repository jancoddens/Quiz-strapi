{
  "kind": "collectionType",
  "collectionName": "game_sessions",
  "info": {
    "singularName": "game-session",
    "pluralName": "game-sessions",
    "displayName": "Game Session"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "code": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "state": {
      "type": "enumeration",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": false,
      "default": "lobby",
      "enum": [
        "lobby",
        "ongoing",
        "finished"
      ]
    },
    "quiz": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::quiz.quiz",
      "inversedBy": "game_sessions"
    },
    "host": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "game_sessions"
    },
    "players": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "game.player",
      "repeatable": true
    },
    "currentQuestionIndex": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": 0
    },
    "settings": {
      "type": "json",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "startedAt": {
      "type": "datetime",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "endedAt": {
      "type": "datetime",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "round": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::round.round",
      "inversedBy": "game_sessions"
    }
  }
}
